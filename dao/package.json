{
  "name": "sngldao",
  "version": "1.0.0",
  "description": "DAO for sngl",
  "scripts": {
    "compile": "truffle compile",
    "deploy-graph": "bash ops/hackyDeploy.sh",
    "ganache": "npm explore @daostack/migration -- npm run ganache",
    "migrate": "node migrations/DeployDao.js",
    "webpack": "npx webpack --config webpack.config.js",
    "launch:app": "cd peepeth-app && npm i && npm run start",
    "launch:docker": "docker-compose up -d",
    "start": "npm run launch:docker && npm run compile && npm run migrate && echo \"npm run deploy-graph && npm run launch:app\"",
    "stop": "docker-compose down"
  },
  "keywords": [
    "DAO",
    "DAOStack",
    "SNGL",
    "Ethereum",
    "Blockchain"
  ],
  "author": "",
  "license": "",
  "dependencies": {
    "@daostack/arc": "0.0.1-rc.39",
    "@daostack/infra": "0.0.1-rc.17",
    "@daostack/migration": "0.0.1-rc.39-v3",
    "@daostack/subgraph": "0.0.37-3",
    "dotenv": "^8.2.0",
    "node": "^8.10.0",
    "openzeppelin-solidity": "^2.1.3",
    "truffle": "^5.1.13"
  },
  "optionalDependencies": {
    "script": "6.0.3"
  },
  "devDependencies": {
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
